{
    "manifest_version": 2,
    "name": "Tab Reloader",
    "description": "Reload available tabs efficiently",
    "version": "0.1",
    "permissions": ["tabs"],
    "background": {
        "scripts": ["main.js"]
    },
    "icons": {
        "16": "icons/icon.svg",
        "28": "icons/icon.svg",
        "32": "icons/icon.svg",
        "64": "icons/icon.svg",
        "128": "icons/icon.svg"
    },
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.svg",
            "28": "icons/icon.svg",
            "32": "icons/icon.svg",
            "64": "icons/icon.svg",
            "128": "icons/icon.svg"
        }
    }
}
